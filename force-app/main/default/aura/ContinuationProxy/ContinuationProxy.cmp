<aura:component controller="ContinuationController" access="global">
    <aura:attribute name="topic" type="String" default="pimsContinuationCalls" access="global" />
    <aura:attribute name="vfBaseURL" type="String" />
    <aura:method name="invoke" action="{!c.doInvoke}">
        <aura:attribute name="methodName" type="String" />
        <aura:attribute name="methodParams" type="Object" />
        <aura:attribute name="callback" type="Object" />
    </aura:method>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <iframe aura:id="vfFrame" src="/apex/ContinuationPage" frameborder="0" width="1" height="1" />
    <iframe aura:id="vfFrameCommunity" src="/partners/ContinuationPage" frameborder="0" width="1" height="1" />
</aura:component>