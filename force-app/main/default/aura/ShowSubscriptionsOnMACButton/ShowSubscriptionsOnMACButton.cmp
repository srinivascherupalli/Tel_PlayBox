                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">{!v.popupHeading}</h2>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <lightning:select aura:id="reasonPicklist" value="{!v.selectedReason}" onchange="{!c.handleReasonChange}" name="reasonPicklist" >
                            <option value="">--Select--</option>
                            <aura:iteration items="{!v.reasonMap}" var="option">
                                <option  value="{!option.key}">{!option.value}</option>
                            </aura:iteration>
                        </lightning:select>
                        
                    </div>
                        
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        
                        
                     <div class="demo-only demo-only--sizing slds-grid">
