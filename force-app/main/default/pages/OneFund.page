                            baseLiveAgentURL: '{!$Label.BaseLiveAgentURL}',
                            eswLiveAgentDevName: '{!$Label.ESWLiveAgentDevName_OneFund}',
                            isOfflineSupportEnabled: false
                        }
                    );
                };
            
                       
            if (!window.embedded_svc) {
                var s = document.createElement('script');
                s.setAttribute('src', '{!$Label.ESWMinJS}');
                s.onload = function() {
                    initESW(null);
                };
                document.body.appendChild(s);
            } else {
