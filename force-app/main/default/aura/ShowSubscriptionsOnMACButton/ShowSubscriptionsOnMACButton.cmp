                        <!--- Subscriptions table--> 
                        <div >

                                <!--EDGE-205598 Start Show Table with pagination for Change of Billing Account-->
                            <aura:if isTrue="{!v.actionTypeSelected =='Change of Billing Account'}">

                                        <c:subscriptionTableComp subids="{!v.subids}"
